<h1>Contacts</h1>
<%= form_tag "contacts_dashboard", :method => 'get', :id => "contacts_search" do %>
  <%= hidden_field_tag :destroy %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <input type="button" value="Clear" id="clear">
    <a onclick="selectAll()" class="check">Check All </a><a onclick="uncheckAll()" class="uncheck">Uncheck All </a>
  </p>
  <div id="contacts"><%= render '/contacts/contacts_list' %></div>
<% end %>

<p><%= link_to "New Contact", new_contact_path,:id=>"new_c" %></p>